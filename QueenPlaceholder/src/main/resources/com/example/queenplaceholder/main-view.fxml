<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.ToggleButton?>
<VBox fx:id="root" alignment="CENTER" spacing="5.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.queenplaceholder.controller.MainController">
    <children>
        <Canvas fx:id="canvas" width="560.0" height="560.0"/>
        <Button fx:id="nextButton" text="Next"/>
        <ToggleButton fx:id="findButton"  text="Find"/>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
</VBox>
