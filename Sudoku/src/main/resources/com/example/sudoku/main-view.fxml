<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.chart.NumberAxis?>
<VBox fx:id="root" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.example.sudoku.controller.MainController" spacing="20.0" alignment="CENTER">
    <padding>
        <Insets top="20.0" bottom="20.0" left="20.0" right="20.0"/>
    </padding>
    <Canvas fx:id="sudokuCanvas" focusTraversable="true" height="540.0" width="540.0"/>
    <Canvas fx:id="numberCanvas" focusTraversable="true" height="30.0" width="30.0"/>
    <Button fx:id="solveButton" mnemonicParsing="false" text="Solve"/>
</VBox>
